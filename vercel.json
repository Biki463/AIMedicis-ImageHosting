{
  "buildCommand": "pip install -r requirements.txt",
  "framework": "fastapi",
  "public": false,
  "functions": {
    "main.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . || exit 0"
}
